<div class="after-footer"></div>
<footer id="player" class="alaire">
	<svg viewBox="0 0 50 50" class="album"><use xlink:href="#album"></use></svg>
	<div class="audio-title">
		<div class="showname">{{radio.title}} {{radio.duration}}</div>
		{{!-- <div class="breadcrumbs">Hunting » Fishlights » Serpientes EP</div> --}}
		<div class="breadcrumbs">
			<span id="insert_title" class="insert_here"></span>
			<span id="insert_artist" class="insert_here"></span>
			<span id="insert_album" class="insert_here"></span>
		</div>
	</div>
	{{# if playing}}
		<i class="material-icons">pause</i>
	{{else}}
		<i class="material-icons">play</i>
	{{/ if}}
	
</footer>